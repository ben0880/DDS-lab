// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
struct Node{
    int val;
    struct Node *next;
    //struct Node*prev;
    
};
class LL{
    public:
    struct Node *start;
    //struct Node *end;
    struct Node *getnode(){
        struct Node*p;
        p=new(struct Node);
        cout<<"give value for new node"<<endl;
        cin>>p->val;
        p->next=NULL;
        //p->prev=NULL;
        return p;
    } 
    
    
    
};
int main(){
        LL b;
        b.start=b.getnode();
        return 0;
  }

